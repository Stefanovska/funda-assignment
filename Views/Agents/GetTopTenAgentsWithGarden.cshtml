@model IEnumerable<Agent>
@{
    ViewData["Title"] = "Agents with garden";
}

<h1>Agents with the most properties for sale with a garden</h1>

<table>
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumberOfProperties)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var b in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => b.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => b.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => b.NumberOfProperties)
            </td>
        </tr>
        }
    </tbody>
</table>
